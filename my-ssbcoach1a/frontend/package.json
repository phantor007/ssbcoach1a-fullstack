{
  "name": "ssbcoach1a-frontend",
  "version": "1.0.0",
  "description": "SSB Coach 1A - Frontend Application (Node.js + Express + EJS)",
  "main": "app.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --watch",
    "watch:js": "webpack --mode development --watch",
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --minify",
    "build:js": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "rm -rf node_modules package-lock.json",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "ssb",
    "defence",
    "interview",
    "preparation",
    "nodejs",
    "express",
    "mongodb",
    "education"
  ],
  "author": "SSB Coach 1A Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "express-session": "^1.17.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "validator": "^13.11.0",
    "express-flash": "^0.0.2",
    "connect-flash": "^0.1.1",
    "express-fileupload": "^1.4.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "chart.js": "^4.4.0",
    "chartjs-adapter-moment": "^1.0.1",
    "datatables.net": "^1.13.8",
    "jquery": "^3.7.1",
    "bootstrap": "^5.3.2",
    "font-awesome": "^4.7.0",
    "aos": "^2.3.4",
    "swiper": "^11.0.5",
    "alpinejs": "^3.13.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "mini-css-extract-plugin": "^2.7.6",
    "terser-webpack-plugin": "^5.3.9",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-loader": "^9.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ssbcoach1a/frontend"
  },
  "bugs": {
    "url": "https://github.com/ssbcoach1a/frontend/issues"
  },
  "homepage": "https://ssbcoach1a.com",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "node_modules/*"
    ],
    "ext": "js,ejs,json"
  }
}